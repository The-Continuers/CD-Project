//If

int main(){

    int x=0;
    int y=2;
    if(x==y){
        Print(2*x);
    }

    if(x>=y){
        Print(2*x);
    } else {
        Print(y+x);
    }

    if(y-1==x+1){
        Print(y);
    } else {
        Print(1);
    }

    if(2*x==2*y){
        Print("ridi");
    } else{
        Print("okeye");
    }
}