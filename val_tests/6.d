// Print
int main() {
    int x;
    double y;
    string z1;
    string z2;
    bool t;
    // int
    x = 4;
    Print( 3);
    Print( x);
    Print( 2 * x);
    
    // double
    y = 5.0;
    Print( 3.0);
    Print( 3.0 * y);
    Print( 10.0 / y);
    
    // string
    z1 = "salam";
    z2 = "khooobi";
    Print( z1);
    Print( z1, z2);
    
    // bool
    t = false;
    Print( ! t);
    Print( t && true);
}